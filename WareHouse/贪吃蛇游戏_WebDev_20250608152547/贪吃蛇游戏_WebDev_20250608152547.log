        # Display the score
        font = pygame.font.SysFont('Arial', 25)
        score_surface = font.render(f'Score: {self.snake.score}', True, (255, 255, 255))
        self.screen.blit(score_surface, (10, 10))
        pygame.display.flip()  # Update the display
